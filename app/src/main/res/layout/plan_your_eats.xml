        android:src="@drawable/default_square"
        app:layout_constraintEnd_toStartOf="@+id/defaultSquare8"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/defaultSquare5" />

    <ImageView
        android:id="@+id/defaultSquare8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="44dp"
        android:contentDescription="@null"
        android:src="@drawable/default_square"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/defaultSquare6" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="45dp"
        android:layout_height="39dp"
        android:src="@drawable/person_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/defaultSquare7" />

    <TextView
        android:id="@+id/date1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/relay_inter_bold"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/defaultSquare1"
        app:layout_constraintEnd_toEndOf="@+id/defaultSquare1"
        app:layout_constraintStart_toStartOf="@+id/defaultSquare1"
        app:layout_constraintTop_toTopOf="@+id/defaultSquare1" />

    <TextView
